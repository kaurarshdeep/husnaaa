$(function() {
document.getElementById("first").onkeyup = function() 
{
myFunction();
splitfunc();
}
});
function myFunction() {

f = document.getElementById("first").value;
document.getElementById("second").value = f;
}

function splitfunc() {
     
   a = f.split(" ");
console.log(a);
    document.getElementById("second").innerHTML = a;
}
   
    


